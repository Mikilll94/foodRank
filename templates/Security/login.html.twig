{% if error %}
    <div>{{ error.messageKey|trans(error.messageData, 'security') }}</div>
{% endif %}

<form class="form" role="form" method="post" action="{{ path('login') }}" accept-charset="UTF-8" id="login-nav">
    <div class="form-group">
        <label class="sr-only" for="exampleInputEmail2">Login</label>
        <input id="username" class="form-control" placeholder="Login" required
               name="_username" value="{{ last_username }}"
               style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%;">
    </div>
    <div class="form-group">
        <label class="sr-only" for="password">Hasło</label>
        <input type="password" class="form-control" id="password" placeholder="Hasło" required
               name="_password"
               style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%;">
        <div class="help-block text-right"><a href="">Zapomaniałeś hasła?</a></div>
    </div>
    <div class="form-group">
        <button type="submit" class="btn btn-primary btn-block">Zaloguj się</button>
    </div>
    <div class="checkbox">
        <label>
            <input type="checkbox"> pamiętaj mnie
        </label>
    </div>
</form>